{
  "version": 2,
  "name": "video-production-pipeline",
  "builds": [
    {
      "src": "src/api/**/*.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/src/api/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "src/api/**/*.ts": {
      "memory": 3008,
      "maxDuration": 300
    }
  }
}
